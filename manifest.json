{
    "name": "Clear History Chrome",
    "version": "1.0",
    "description": "Clear history Chrome.",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "history",
        "declarativeContent"
    ],
    "icons": {
        "128": "assets/icon128.png"
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}